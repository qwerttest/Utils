<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wj.base.utils.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wj.base.utils.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.wj.base.utils.demo.ApplicationInfoUtilsActivity"/>
        <activity android:name="com.wj.base.utils.demo.CookiesUtilsActivity"/>
        <activity android:name="com.wj.base.utils.demo.DensityUtilsActivity"/>
        <activity android:name="com.wj.base.utils.demo.DeviceUtilsActivity"/>
        <activity android:name="com.wj.base.utils.demo.KeyBoardUtilsActivity"/>
        <activity android:name="com.wj.base.utils.demo.PrompUtilsActivity"/>
        <activity android:name="com.wj.base.utils.demo.RegularUtilsActivity"/>
    </application>

</manifest>